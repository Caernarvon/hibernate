<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="items.Region" table="RegionH">
        <meta attribute="class-description">
            This class contains developer's details.
        </meta>
        <id name="id" type="int" column="id"/>
        <property name="name" column="name" type="string"/>
        <property name="code" column="code" type="int"/>
        <property name="populationAmount" column="populationAmount" type="int"/>
        <property name="geoData" column="geoData" type="string"/>
        <property name="emblemId" column="emblemId" type="int"/>
    </class>
</hibernate-mapping>